msgid "Available"
msgstr "Tilgængelig"

#: ../libempathy/empathy-utils.c:182
msgid "Busy"
msgstr "Optaget"

#: ../libempathy/empathy-utils.c:185
msgid "Away"
msgstr "Ikke til stede"

#: ../libempathy/empathy-utils.c:187
msgid "Invisible"
msgstr "Usynlig"

#: ../libempathy/empathy-utils.c:189
msgid "Offline"
msgstr "Offline"

