#, c-format
msgid "Call from %s"
msgstr "Apel de la %s"

#: ../libempathy/empathy-utils.c:180
msgid "Available"
msgstr "Disponibil"

#: ../libempathy/empathy-utils.c:182
msgid "Busy"
msgstr "Ocupat"

#: ../libempathy/empathy-utils.c:185
msgid "Away"
msgstr "Plecat"

